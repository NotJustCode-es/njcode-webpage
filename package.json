{
  "name": "njcode-webpage",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/njcode-webpage/server/main.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=true --code-coverage",
    "test:ci": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "server:test": "jest --config=jest.json",
    "server:test:ci": "jest --config=jest.json --coverage=true",
    "lint": "eslint -c .eslintrc.json --ext .ts .",
    "dev:ssr": "npm run server:copy:templates && ng run njcode-webpage:serve-ssr",
    "serve:ssr": "node dist/njcode-webpage/server/main.js",
    "build:ssr": "ng build --configuration production && ng run njcode-webpage:server:production && npm run server:copy:templates",
    "prerender": "ng run njcode-webpage:prerender",
    "server:copy:templates": "mkdir -p dist/njcode-webpage/server/templates && cp -R server/templates dist/njcode-webpage/server/templates"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.12",
    "@angular/common": "~14.2.12",
    "@angular/compiler": "~14.2.12",
    "@angular/core": "~14.2.12",
    "@angular/forms": "~14.2.12",
    "@angular/localize": "~14.2.12",
    "@angular/platform-browser": "~14.2.12",
    "@angular/platform-browser-dynamic": "~14.2.12",
    "@angular/platform-server": "~14.2.12",
    "@angular/router": "~14.2.12",
    "@contentful/rich-text-html-renderer": "^16.0.3",
    "@html-eslint/eslint-plugin": "~0.16.0",
    "@html-eslint/parser": "~0.16.0",
    "@nestjs-modules/mailer": "~1.8.1",
    "@nestjs/axios": "~1.0.1",
    "@nestjs/common": "~8.4.7",
    "@nestjs/config": "~2.2.0",
    "@nestjs/core": "~8.4.7",
    "@nestjs/ng-universal": "~6.1.0",
    "@nestjs/platform-express": "~8.4.7",
    "@nestlab/google-recaptcha": "~3.2.0",
    "@ng-bootstrap/ng-bootstrap": "~13.1.1",
    "@ngneat/transloco": "~4.2.1",
    "@nguniversal/express-engine": "~14.2.3",
    "@popperjs/core": "~2.11.6",
    "autoprefixer": "~10.4.5",
    "bootstrap": "~5.2.2",
    "cache-manager": "~4.1.0",
